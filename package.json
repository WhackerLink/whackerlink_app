{
  "name": "whackerlink_app",
  "author": "PHP",
  "version": "0.1.0",
  "description": "whacker desktop",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.whackerlink.whackerlink-app",
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "devDependencies": {
    "electron": "^26.2.0",
    "electron-builder": "^24.6.4"
  }
}
